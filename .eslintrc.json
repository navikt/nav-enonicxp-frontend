{
    "plugins": ["css-modules", "@typescript-eslint"],
    "extends": [],
    "rules": {
        "css-modules/no-unused-class": [
            1,
            {
                "camelCase": true
            }
        ],
        "css-modules/no-undef-class": [
            2,
            {
                "camelCase": true
            }
        ],
        "@typescript-eslint/no-unused-vars": [
            "warn",
            {
                "varsIgnorePattern": "^_",
                "argsIgnorePattern": "^_",
                "caughtErrorsIgnorePattern": "^_"
            }
        ]
    }
}
