{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "lib": ["es2023"],
        "moduleResolution": "bundler",
        "module": "esnext",
        "noEmit": true
    },
    "include": ["src/**/*.ts", "../common/**/*.ts", "../nodeenv.d.ts"]
}
