    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
