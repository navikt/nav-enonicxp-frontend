        "lint:tsc": "tsc",
        "lint:eslint": "eslint src",
        "test": "TZ=Europe/Oslo jest --testPathIgnorePatterns=server/src"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix --max-warnings=0"
        ],
        "*.{ts,tsx,css,scss}": [
            "prettier --write"
        ]
    },
