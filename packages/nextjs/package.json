{
    "name": "nav-enonicxp-frontend-nextjs",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "next build",
        "build:storybook": "storybook build",
        "prestorybook": "node playwright/copy-screenshots.js",
        "storybook": "storybook dev -p 6006",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build",
