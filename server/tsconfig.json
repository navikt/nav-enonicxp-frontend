{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "skipLibCheck": true,
    "outDir": "../.serverDist"
  },
  "include": [
    "./**/*.ts",
  ],
}
