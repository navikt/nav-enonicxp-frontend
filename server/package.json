{
    "name": "nav-enonicxp-frontend-server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node -r dotenv/config ../.serverDist/server.js dotenv_config_path=../.env",
        "nodemon-start": "nodemon -r dotenv/config -w ../.serverDist ../.serverDist/server.js dotenv_config_path=../.env",
        "dev": "npm run build && concurrently \"npm run build -- --watch\" \"npm run nodemon-start\""
    },
    "nodemonConfig": {
        "delay": 500
    },
    "dependencies": {
        "cookie-parser": "1.4.6",
        "express-prom-bundle": "6.6.0",
        "http-terminator": "3.2.0",
        "on-headers": "1.0.2",
        "prom-client": "14.2.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "1.4.4",
        "@types/express": "4.17.19",
        "@types/on-headers": "1.0.3",
        "concurrently": "7.6.0",
        "express": "4.18.2",
        "nodemon": "3.0.2"
    }
}
