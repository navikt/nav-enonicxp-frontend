{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "tsc -p server && next build",
        "start": "node .serverDist/server.js",
        "dev": "cp .env.development .env && tsc -p server && npm start",
        "dev-custom": "cp .env.development.local .env && tsc -p server && npm start",
        "dev-server": "cp .env.prod-local.local .env && concurrently \"tsc -w -p server\" \"nodemon -w .serverDist .serverDist/server.js\"",
        "build-local": "cp .env.prod-local .env && npm run build",
        "start-local": "cp .env.prod-local .env && npm run start",
        "start-clean": "rm -fr .next && npm run build-local && npm run start-local",
        "build-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true npm run build-local",
        "start-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true PORT=3003 npm run start",
        "start-clean-failover": "rm -fr .next-static && npm run build-local-failover && npm run start-local-failover",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "test-server": "next build server/__next-test-dummy && NODE_OPTIONS=--experimental-vm-modules jest --config server/jest.config.mjs --testPathPattern=server/",
        "test-client": "TZ=Europe/Oslo jest --testPathPattern=src/",
        "test": "npm run test-server && npm run test-client",
        "prepare": "husky install"
    },
    "dependencies": {
        "@grafana/faro-web-sdk": "1.2.9",
        "@grafana/faro-web-tracing": "1.2.9",
        "@navikt/aksel-icons": "5.11.3",
        "@navikt/ds-css": "5.17.5",
        "@navikt/ds-react": "5.17.5",
        "@navikt/ds-tokens": "5.17.5",
        "@navikt/nav-dekoratoren-moduler": "2.1.5",
        "@reduxjs/toolkit": "1.9.7",
        "cookie-parser": "1.4.6",
        "csp-header": "5.2.1",
        "dayjs": "1.11.10",
        "dotenv": "16.3.1",
        "express-prom-bundle": "6.6.0",
        "fuse.js": "6.6.2",
        "html-react-parser": "5.0.7",
        "http-terminator": "3.2.0",
        "js-cookie": "3.0.5",
        "jsdom": "23.0.1",
        "jsep": "1.3.8",
        "lodash.debounce": "4.0.8",
        "lottie-web": "5.12.2",
        "lru-cache": "10.1.0",
        "next": "14.0.4",
        "node-cache": "5.1.2",
        "on-headers": "1.0.2",
        "p-limit": "5.0.0",
        "prom-client": "14.2.0",
        "react": "18.2.0",
        "react-collapse": "5.1.1",
        "react-dom": "18.2.0",
        "react-movable": "3.0.4",
        "react-redux": "8.1.3",
        "redis": "4.6.13",
        "rss": "1.2.2",
        "sharp": "0.33.0",
        "swr": "2.2.4",
        "tough-cookie": "4.1.3",
        "uuid": "9.0.1",
        "word-wrap": "1.2.5"
    },
    "dependenciesComments": {
        "tough-cookie": "Transitive dependency only, bumped directly due to vulnerability alert",
        "word-wrap": "Transitive dependency only, bumped directly due to vulnerability alert"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "14.0.4",
        "@next/eslint-plugin-next": "14.0.4",
        "@testing-library/jest-dom": "6.1.4",
        "@testing-library/react": "14.1.2",
        "@types/cookie-parser": "1.4.4",
        "@types/express": "4.17.19",
        "@types/jest": "29.5.10",
        "@types/mock-fs": "4.13.4",
        "@types/node": "20.10.3",
        "@types/on-headers": "1.0.1",
        "@types/react": "18.2.28",
        "@types/react-collapse": "5.0.4",
        "@types/react-dom": "18.2.13",
        "@types/react-redux": "7.1.31",
        "@types/rss": "0.0.32",
        "@types/uuid": "9.0.7",
        "@typescript-eslint/eslint-plugin": "5.59.6",
        "@typescript-eslint/parser": "5.59.6",
        "babel-eslint": "10.1.0",
        "concurrently": "7.6.0",
        "eslint": "8.55.0",
        "eslint-config-next": "14.0.4",
        "eslint-plugin-css-modules": "2.12.0",
        "express": "4.18.2",
