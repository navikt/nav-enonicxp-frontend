{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "next build",
        "start": "node server/server.js",
        "dev": "cp .env.development .env && npm start",
        "start-local": "cp .env.local .env && npm start",
        "start-clean": "rm -fr .next && npm run build && npm run start-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "deploy": "npx run-func trigger-workflow.js triggerWorkflow",
        "deploy-dev-gcp": "npm run deploy -- deploy.dev-gcp.yml",
        "deploy-dev": "npm run deploy -- deploy.dev.yml",
        "deploy-q6": "npm run deploy -- deploy.q6.yml"
    },
    "dependencies": {
        "@navikt/ds-css": "^0.12.14",
        "@navikt/ds-icons": "^0.7.3",
        "@navikt/ds-react": "^0.14.15",
        "@navikt/ds-tokens": "^0.7.1",
        "@navikt/fnrvalidator": "^1.1.4",
        "@navikt/nav-dekoratoren-moduler": "^1.6.9",
        "@reduxjs/toolkit": "^1.7.1",
        "amplitude-js": "^8.16.0",
        "classnames": "^2.3.1",
        "dayjs": "^1.10.7",
        "dotenv": "^11.0.0",
        "express": "^4.17.2",
        "html-react-parser": "^1.4.5",
        "jsdom": "^16.7.0",
        "immer": "^9.0.12",
        "lodash": "^4.17.21",
        "lodash.debounce": "^4.0.8",
        "lottie-web": "^5.8.1",
        "mathjs": "^10.1.0",
        "next": "^12.0.8",
        "next-compose-plugins": "^2.2.1",
        "next-images": "^1.8.4",
        "next-transpile-modules": "^9.0.0",
        "next-with-less": "^2.0.4",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.7",
        "on-headers": "^1.0.2",
        "parse-domain": "^4.1.0",
        "prom-client": "^14.0.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-collapse": "^5.1.1",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-movable": "2.5.4",
        "ssri": "6.0.2"
    },
    "devDependencies": {
        "@next/eslint-plugin-next": "^12.0.8",
        "@octokit/core": "^3.5.1",
        "@types/amplitude-js": "^8.9.3",
        "@types/node": "^16.11.19",
        "@types/react": "^17.0.38",
        "@types/react-collapse": "^5.0.1",
        "@types/react-dom": "^17.0.11",
        "@types/react-redux": "^7.1.22",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.6.0",
        "eslint-config-next": "^12.0.8",
        "husky": "^7.0.4",
        "less": "^3.13.1",
        "lint-staged": "^12.1.7",
        "prettier": "^2.5.1",
        "sass": "^1.48.0",
        "typescript": "^4.5.4",
        "typescript-plugin-css-modules": "^3.4.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run typeCheck && npm run lint"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,less,css}": "prettier --write"
    }
}
