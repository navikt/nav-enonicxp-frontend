{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "next build && tsc -p server",
        "start": "node .serverDist/server.js",
        "dev": "cp .env.development .env && tsc -p server && npm start",
        "dev-server": "cp .env.prod-local .env && concurrently \"tsc -w -p server\" \"nodemon -w .serverDist .serverDist/server.js\"",
        "build-local": "cp .env.prod-local .env && npm run build",
        "start-local": "cp .env.prod-local .env && npm run start",
        "start-clean": "rm -fr .next && npm run build-local && npm run start-local",
        "build-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true npm run build-local",
        "start-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true PORT=3003 npm run start",
        "start-clean-failover": "rm -fr .next-static && npm run build-local-failover && npm run start-local-failover",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "deploy": "npx run-func trigger-workflow.js triggerWorkflow",
        "deploy-dev1": "npm run deploy -- deploy.dev1.yml",
        "deploy-dev2": "npm run deploy -- deploy.dev2.yml",
        "test-server": "cd server && next build __next-test-dummy && jest"
    },
    "dependenciesComments":{
        "@sentry/nextjs": "[7.7.0] Newer versions breaks the failover-html response in our _error page. Needs to be solved before upgrading."
    },
    "dependencies": {
        "@navikt/ds-css": "1.5.6",
        "@navikt/ds-icons": "1.5.6",
        "@navikt/ds-react": "1.5.6",
        "@navikt/ds-tokens": "1.5.6",
        "@navikt/nav-dekoratoren-moduler": "1.8.1",
        "@reduxjs/toolkit": "1.9.0",
        "@sentry/integrations": "7.7.0",
        "@sentry/nextjs": "7.7.0",
        "amplitude-js": "8.21.2",
        "classnames": "2.3.2",
        "csp-header": "5.1.0",
        "dayjs": "1.11.6",
        "dotenv": "16.0.3",
        "express": "4.18.2",
        "html-react-parser": "3.0.4",
        "http-terminator": "3.2.0",
        "js-cookie": "3.0.1",
        "jsdom": "20.0.2",
        "lodash.debounce": "4.0.8",
        "lottie-web": "5.9.6",
        "lru-cache": "7.13.1",
        "mathjs": "10.6.4",
        "next": "12.3.1",
        "node-cache": "5.1.2",
        "node-fetch": "2.6.7",
        "on-headers": "1.0.2",
        "p-limit": "4.0.0",
        "prom-client": "14.1.0",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-collapse": "5.1.1",
        "react-dom": "18.2.0",
        "react-movable": "2.5.4",
        "react-redux": "8.0.5",
        "rss": "1.2.2",
        "sharp": "0.31.2",
        "ssri": "6.0.2",
        "swr": "1.3.0",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "12.3.1",
        "@next/eslint-plugin-next": "12.3.1",
        "@octokit/core": "4.0.4",
        "@types/amplitude-js": "8.16.2",
        "@types/express": "4.17.14",
        "@types/jest": "29.2.3",
        "@types/mock-fs": "4.13.1",
        "@types/node-fetch": "2.6.2",
        "@types/node": "16.11.44",
        "@types/on-headers": "1.0.0",
        "@types/react-collapse": "5.0.1",
        "@types/react-dom": "18.0.6",
        "@types/react-redux": "7.1.24",
        "@types/react": "18.0.15",
        "@typescript-eslint/eslint-plugin": "5.30.6",
        "@typescript-eslint/parser": "5.30.6",
        "babel-eslint": "10.1.0",
        "concurrently": "7.5.0",
        "eslint-config-next": "12.3.1",
        "eslint-plugin-css-modules": "2.11.0",
        "eslint": "8.26.0",
        "husky": "8.0.1",
        "jest": "29.3.1",
        "jest-fetch-mock": "3.0.3",
        "lint-staged": "13.0.3",
        "mock-fs": "5.2.0",
        "nodemon": "2.0.20",
        "prettier": "2.7.1",
        "sass": "1.56.0",
        "ts-jest": "29.0.3",
        "typescript-plugin-css-modules": "3.4.0",
        "typescript": "4.8.4"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run typeCheck && npm run lint"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
}
