{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "next build",
        "start": "node server/server.js",
        "dev": "cp .env.development .env && npm start",
        "build-local": "cp .env.prod-local .env && npm run build",
        "start-local": "cp .env.prod-local .env && npm run start",
        "start-clean": "rm -fr .next && npm run build-local && npm run start-local",
        "build-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true npm run build-local",
        "start-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true PORT=3003 npm run start",
        "start-clean-failover": "rm -fr .next-static && npm run build-local-failover && npm run start-local-failover",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "deploy": "npx run-func trigger-workflow.js triggerWorkflow",
        "deploy-dev1": "npm run deploy -- deploy.dev1.yml",
        "deploy-dev2": "npm run deploy -- deploy.dev2.yml"
    },
    "dependencies": {
        "@navikt/ds-css": "^0.16.5",
        "@navikt/ds-icons": "^0.8.4",
        "@navikt/ds-react": "^0.16.7",
        "@navikt/ds-tokens": "^0.8.3",
        "@navikt/fnrvalidator": "^1.1.4",
        "@navikt/nav-dekoratoren-moduler": "^1.6.17-beta.0",
        "@reduxjs/toolkit": "^1.8.0",
        "amplitude-js": "^8.17.0",
        "classnames": "^2.3.1",
        "dayjs": "^1.10.7",
        "dotenv": "^11.0.0",
        "express": "^4.17.3",
        "html-react-parser": "^1.4.8",
        "immer": "^9.0.12",
        "js-cookie": "^3.0.1",
        "jsdom": "^16.7.0",
        "lodash": "^4.17.21",
        "lodash.debounce": "^4.0.8",
        "lottie-web": "^5.8.1",
        "mathjs": "^10.6.4",
        "next": "^12.1.6",
        "next-compose-plugins": "^2.2.1",
        "next-transpile-modules": "^9.0.0",
        "node-cache": "^5.1.2",
        "node-fetch": "^2.6.7",
        "on-headers": "^1.0.2",
        "p-limit": "^4.0.0",
        "prom-client": "^14.0.1",
        "prop-types": "^15.8.1",
        "react": "^17.0.2",
        "react-collapse": "^5.1.1",
        "react-dom": "^17.0.2",
        "react-movable": "2.5.4",
        "react-redux": "^7.2.6",
        "rss": "^1.2.2",
        "sharp": "^0.30.7",
        "ssri": "6.0.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.1.6",
        "@next/eslint-plugin-next": "^12.1.6",
        "@octokit/core": "^3.5.1",
        "@types/amplitude-js": "^8.9.3",
        "@types/mathjs": "^9.4.2",
        "@types/node": "^16.11.19",
        "@types/react": "^17.0.38",
        "@types/react-collapse": "^5.0.1",
        "@types/react-dom": "^17.0.11",
        "@types/react-redux": "^7.1.22",
        "@typescript-eslint/eslint-plugin": "^5.28.0",
        "@typescript-eslint/parser": "^5.28.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.6.0",
        "eslint-config-next": "^12.1.6",
        "eslint-plugin-css-modules": "^2.11.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.7",
        "prettier": "^2.5.1",
        "sass": "^1.48.0",
        "typescript": "^4.7.3",
        "typescript-plugin-css-modules": "^3.4.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run typeCheck && npm run lint"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
}
