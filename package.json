        "typescript": "5.3.2",
        "typescript-plugin-css-modules": "5.0.2"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
