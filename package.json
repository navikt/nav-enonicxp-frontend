{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "npm run build:server && npm run build:nextjs",
        "build:nextjs": "npm run build --workspace packages/nextjs",
        "build:server": "npm run build --workspace packages/server",
        "start": "npm run start --workspace server",
        "dev": "npm run build --workspace packages/server && npm run start --workspace packages/server",
        "dev-custom": "npm run dev-custom --workspace packages/nextjs",
        "dev-server": "npm run dev-server --workspace packages/nextjs",
        "build-local": "npm run build-local --workspace packages/nextjs",
        "start-local": "npm run start-local --workspace packages/nextjs",
        "start-clean": "npm run start-clean --workspace packages/nextjs",
        "build-local-failover": "npm run build-local-failover --workspace packages/nextjs",
        "start-local-failover": "npm run start-local-failover --workspace packages/nextjs",
        "start-clean-failover": "npm run start-clean-failover --workspace packages/nextjs",
        "analyze-bundle": "npm run analyze-bundle --workspace packages/nextjs",
        "lint": "npm-run-all --parallel lint:*",
        "lint:client": "npm run lint --workspace packages/nextjs",
        "lint:server": "npm run lint --workspace packages/server",
        "test": "npm-run-all --parallel test:*",
        "test:server": "npm run test --workspace packages/server",
        "test:client": "TZ=Europe/Oslo jest --testPathIgnorePatterns=server/src",
        "prepare": "husky install",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "@navikt/nav-dekoratoren-moduler": "3.1.0",
        "next": "14.2.10",
        "redis": "4.6.15"
    },
    "devDependencies": {
        "@types/node": "20.14.10",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "esbuild": "0.23.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-nibble": "8.1.0",
        "eslint-plugin-css-modules": "2.12.0",
        "eslint-plugin-no-relative-import-paths": "1.5.5",
        "lint-staged": "15.2.7",
        "prettier": "^3.3.3",
        "typescript": "5.5.3",
        "typescript-plugin-css-modules": "5.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix packages/nextjs packages/server packages/shared"
        ],
        "*.{scss,css}": [
            "prettier --write"
        ]
    }
}
