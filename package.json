{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "next build",
        "start": "node server/server.js",
        "dev": "cp .env.development .env && npm start",
        "build-local": "cp .env.prod-local .env && npm run build",
        "start-local": "cp .env.prod-local .env && npm run start",
        "start-clean": "rm -fr .next && npm run build-local && npm run start-local",
        "build-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true npm run build-local",
        "start-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true PORT=3003 npm run start",
        "start-clean-failover": "rm -fr .next-static && npm run build-local-failover && npm run start-local-failover",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "deploy": "npx run-func trigger-workflow.js triggerWorkflow",
        "deploy-dev1": "npm run deploy -- deploy.dev1.yml",
        "deploy-dev2": "npm run deploy -- deploy.dev2.yml"
    },
    "dependencies": {
        "@navikt/ds-css": "^0.18.27",
        "@navikt/ds-icons": "^0.8.19",
        "@navikt/ds-react": "^0.19.23",
        "@navikt/ds-tokens": "^0.9.7",
        "@navikt/fnrvalidator": "^1.3.0",
        "@navikt/nav-dekoratoren-moduler": "^1.6.17",
        "@reduxjs/toolkit": "^1.8.3",
        "amplitude-js": "^8.18.5",
        "classnames": "^2.3.1",
        "dayjs": "^1.11.3",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "html-react-parser": "^3.0.1",
        "js-cookie": "^3.0.1",
        "jsdom": "^20.0.0",
        "lodash.debounce": "^4.0.8",
        "lottie-web": "^5.9.6",
        "mathjs": "^10.6.4",
        "next": "^12.2.2",
        "next-compose-plugins": "^2.2.1",
        "next-transpile-modules": "^9.0.0",
        "node-cache": "^5.1.2",
        "node-fetch": "2.6.7",
        "on-headers": "^1.0.2",
        "p-limit": "^4.0.0",
        "prom-client": "^14.0.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-collapse": "^5.1.1",
        "react-dom": "^18.2.0",
        "react-movable": "2.5.4",
        "react-redux": "^8.0.2",
        "rss": "^1.2.2",
        "sharp": "^0.30.7"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^12.2.2",
        "@next/eslint-plugin-next": "^12.2.2",
        "@octokit/core": "^4.0.4",
        "@types/amplitude-js": "^8.16.2",
        "@types/node": "^16.11.44",
        "@types/react": "^18.0.15",
        "@types/react-collapse": "^5.0.1",
        "@types/react-dom": "^18.0.6",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^5.30.6",
        "@typescript-eslint/parser": "^5.30.6",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.19.0",
        "eslint-config-next": "^12.2.2",
        "eslint-plugin-css-modules": "^2.11.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "sass": "^1.53.0",
        "typescript": "^4.7.4",
        "typescript-plugin-css-modules": "^3.4.0"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run typeCheck && npm run lint"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
}
