{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "npm run build:server && npm run build:next",
        "build:next": "npm run build:next --workspace packages/nextjs",
        "build:server": "npm run build --workspace packages/server",
        "start": "npm run start --workspace packages/server",
        "dev": "npm run dev --workspace packages/nextjs",
        "dev-custom": "npm run dev-custom --workspace packages/nextjs",
        "dev-server": "npm run dev-server --workspace packages/nextjs",
        "build-local": "npm run build-local --workspace packages/nextjs",
        "start-local": "npm run start-local --workspace packages/nextjs",
        "start-clean": "npm run start-clean --workspace packages/nextjs",
        "build-local-failover": "npm run build-local-failover --workspace packages/nextjs",
        "start-local-failover": "npm run start-local-failover --workspace packages/nextjs",
        "start-clean-failover": "npm run start-clean-failover --workspace packages/nextjs",
        "analyze-bundle": "npm run analyze-bundle --workspace packages/nextjs",
        "lint": "npm run lint --workspace packages/nextjs",
        "lint:tsc": "npm run lint:tsc --workspace packages/nextjs",
        "lint:client": "npm run lint:client --workspace packages/nextjs",
        "lint:server": "npm run lint:server --workspace packages/nextjs",
        "test": "npm run test --workspace packages/nextjs",
        "test:server": "npm run test:server --workspace packages/nextjs",
        "test:client": "npm run test:client --workspace packages/nextjs",
        "prepare": "npm run prepare --workspace packages/nextjs",
        "storybook": "npm run storybook --workspace packages/nextjs",
        "build-storybook": "npm run build-storybook --workspace packages/nextjs"
    },
    "workspaces": [
        "packages/*"
    ]
}
