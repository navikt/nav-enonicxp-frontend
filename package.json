        "eslint-nibble": "8.1.0",
        "eslint-plugin-css-modules": "2.12.0",
        "eslint-plugin-no-relative-import-paths": "1.5.5",
        "eslint-plugin-storybook": "0.8.0",
        "husky": "9.0.11",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-fetch-mock": "3.0.3",
        "lint-staged": "15.2.7",
        "mock-fs": "5.2.0",
        "npm-run-all": "4.1.5",
        "pino-pretty": "11.2.1",
        "prettier": "3.3.2",
        "sass": "1.77.6",
        "storybook": "8.2.8",
        "ts-jest": "29.1.5",
        "typescript": "5.5.3",
        "typescript-plugin-css-modules": "5.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix src srcCommon server/src"
        ],
        "*.{scss,css}": [
            "prettier --write"
        ]
    }
}
