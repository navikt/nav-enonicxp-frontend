{
    "name": "nav-enonicxp-frontend",
    "version": "1.2.1",
    "private": true,
    "scripts": {
        "build": "tsc -p server && next build",
        "start": "node .serverDist/server.js",
        "dev": "cp .env.development .env && tsc -p server && npm start",
        "dev-custom": "cp .env.development.local .env && tsc -p server && npm start",
        "dev-server": "cp .env.prod-local .env && concurrently \"tsc -w -p server\" \"nodemon -w .serverDist .serverDist/server.js\"",
        "build-local": "cp .env.prod-local .env && npm run build",
        "start-local": "cp .env.prod-local .env && npm run start",
        "start-clean": "rm -fr .next && npm run build-local && npm run start-local",
        "build-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true npm run build-local",
        "start-local-failover": "cp .env.prod-local .env && IS_FAILOVER_INSTANCE=true PORT=3003 npm run start",
        "start-clean-failover": "rm -fr .next-static && npm run build-local-failover && npm run start-local-failover",
        "analyze-bundle": "ANALYZE_BUNDLE=true npm run build-local",
        "typeCheck": "tsc",
        "lint": "eslint src/",
        "test-server": "next build server/__next-test-dummy && jest --testPathPattern=server/",
        "test-client": "TZ=Europe/Oslo jest --testPathPattern=src/",
        "test": "npm run test-server && npm run test-client",
        "prepare": "husky install"
    },
    "dependencies": {
        "@grafana/faro-web-sdk": "^1.1.3",
        "@grafana/faro-web-tracing": "^1.1.3",
        "@navikt/ds-css": "2.8.11",
        "@navikt/aksel-icons": "4.10.2",
        "@navikt/ds-react": "2.8.11",
        "@navikt/ds-tokens": "2.8.11",
        "@navikt/nav-dekoratoren-moduler": "2.0.1",
        "@reduxjs/toolkit": "^1.9.1",
        "classnames": "^2.3.2",
        "cookie-parser": "1.4.6",
        "csp-header": "^5.1.0",
        "dayjs": "^1.11.7",
        "dotenv": "16.0.3",
        "express": "^4.18.2",
        "express-prom-bundle": "6.6.0",
        "fuse.js": "6.6.2",
        "html-react-parser": "3.0.4",
        "http-terminator": "3.2.0",
        "js-cookie": "3.0.1",
        "jsdom": "^20.0.3",
        "lodash.debounce": "4.0.8",
        "lottie-web": "5.10.2",
        "lru-cache": "8.0.4",
        "mathjs": "10.6.4",
        "next": "13.4.1",
        "node-cache": "5.1.2",
        "on-headers": "1.0.2",
        "p-limit": "4.0.0",
        "prom-client": "14.1.1",
        "react": "18.2.0",
        "react-collapse": "5.1.1",
        "react-dom": "18.2.0",
        "react-movable": "3.0.4",
        "react-redux": "8.0.5",
        "rss": "1.2.2",
        "sharp": "0.32.1",
        "swr": "2.1.5",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "13.4.1",
        "@next/eslint-plugin-next": "13.4.1",
        "@types/cookie-parser": "1.4.3",
        "@types/express": "4.17.14",
        "@types/jest": "29.2.5",
        "@types/mock-fs": "4.13.1",
        "@types/node": "18.14.0",
        "@types/on-headers": "1.0.0",
        "@types/react": "18.0.26",
        "@types/react-collapse": "5.0.1",
        "@types/react-dom": "18.0.10",
        "@types/react-redux": "7.1.24",
        "@types/rss": "0.0.30",
        "@types/uuid": "9.0.2",
        "@typescript-eslint/eslint-plugin": "5.59.6",
        "@typescript-eslint/parser": "5.59.6",
        "babel-eslint": "10.1.0",
        "concurrently": "7.6.0",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.1",
        "eslint-plugin-css-modules": "2.11.0",
        "husky": "^8.0.3",
        "jest": "29.3.1",
        "jest-fetch-mock": "3.0.3",
        "lint-staged": "13.0.3",
        "mock-fs": "5.2.0",
        "nodemon": "2.0.20",
        "prettier": "2.8.7",
        "sass": "1.57.1",
        "ts-jest": "29.1.0",
        "typescript": "5.1.6",
        "typescript-plugin-css-modules": "3.4.0"
    },
    "browserslist": {
        "production": [
            ">0.2% in no",
            "iOS >= 11",
            "safari >= 11",
            "not dead",
            "not op_mini all",
            "not ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "jsdom": false
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
}
